{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app-tailwind.css -o app/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app-tailwind.css -o app/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\" \"npm run dev:wrangler\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@radix-ui/react-context-menu": "^2.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@radix-ui/react-hover-card": "^1.0.2",
    "@radix-ui/react-popover": "^1.0.2",
    "@radix-ui/react-select": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.1",
    "@remix-run/cloudflare": "^1.8.1",
    "@remix-run/cloudflare-pages": "^1.8.1",
    "@remix-run/react": "^1.8.1",
    "@supabase/auth-helpers-remix": "^0.1.3",
    "@supabase/supabase-js": "^2.1.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "framer-motion": "^7.6.19",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.8.1",
    "@remix-run/eslint-config": "^1.8.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "wrangler": "^2.6.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
