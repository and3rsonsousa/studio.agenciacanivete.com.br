{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "remix build && run-p \"dev:*\"",
    "generate:css": "npx tailwindcss -i ./styles/app-tailwind.css -o ./app/app.css",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@radix-ui/react-select": "^1.1.1",
    "@remix-run/cloudflare": "^1.7.4",
    "@remix-run/cloudflare-pages": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@supabase/auth-helpers-remix": "^0.1.0",
    "@supabase/supabase-js": "^2.0.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "framer-motion": "^7.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.27"
  },
  "engines": {
    "node": ">=16.13"
  }
}
